// This is my 14 months boy's face. 
// Before today dinner, I wanted to draw a very cute face. but this evening, he didn't eat his dinner well. 
// So I decided to draw his face like this. 

// Setup
size(500, 500);
background(#FFFFEE);

// Drawing a shape of face
stroke(#E8C4A9);
fill(#E8D4B9);
ellipse(250, 250, 400, 400);

// hair 
stroke(#001111);
strokeWeight(5);
line(250, 50, 250, 150);
line(240, 52, 220, 150);
line(230, 54, 200, 150);
line(220, 56, 170, 150);
line(210, 58, 140, 150);
line(200, 60, 110, 150);
line(190, 62, 80, 150);
line(260, 52, 280, 150);
line(270, 54, 310, 150);
line(280, 56, 340, 150);
line(290, 58, 370, 150);
line(300, 60, 400, 150);
line(310, 62, 430, 150);

// Eyes
stroke(#220066);
strokeWeight(7);
arc(160, 230, 70, 70, 3.14, 6.28);
arc(340, 230, 70, 70, 3.14, 6.28);

// nose
fill(#664433);
triangle(250, 270, 220, 320, 280, 320);

// mouse
stroke(#F5180C);
fill(#FF9690);
arc(250, 350, 70, 70, 0, 3.14);







